<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.meowmusicplayer">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SongList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SongList"
            android:label="@string/list_songs" />
        <activity
            android:name=".AlbumList"
            android:label="@string/list_artists" />
        <activity
            android:name=".ArtistList"
            android:label="@string/list_albums" />
        <activity
            android:name=".Playlists"
            android:label="@string/list_playlists" />
        <activity android:name=".FragmentPagerSupport" />
        <activity
            android:name=".menu"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme">

        </activity>
    </application>

</manifest>